<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/btn_do"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="set text"
        />

    <Button
        android:id="@+id/btn_a1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="A class static a()"
        />

    <Button
        android:id="@+id/btn_a2"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="A class static aa(int)"
        />

    <Button
        android:id="@+id/btn_b1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="B class b()"
        />

    <Button
        android:id="@+id/btn_b2"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="B class bb(int)"
        />

    <TextView
        android:id="@+id/tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="111"
        />
</LinearLayout>